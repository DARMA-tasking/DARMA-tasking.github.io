        <type>int8_t</type>
        <name>eEpochHeader</name>
        <enumvalue id="namespacevt_1_1epoch_1a8cbcd2bf5d3ce987837869c26a1d00ada4bbf8ef3641b58f81dc5320f32df0178" prot="public">
